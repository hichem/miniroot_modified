# Target options
TARGET = x86
TARGET_VARIANT = i686
TARGET_ABI = 
TARGET_ENDIANNESS = 
TARGET_FLOAT = hardware

# Toolchain options
SYSROOT = yes
BUILD_SHARED_LIBS = yes
MANUFACTURER = unknown

# OS
TARGET_OS = linux
KERNEL_VERSION = 2.6.27

# GMP - MPFR
GMP_MPFR = yes
GMP_VERSION = 4.2.4
MPFR_VERSION = 2.3.2

# BINUTILS
BINUTILS_VERSION = 2.19
LIBBFD = yes
LIBIBERTY = yes

# C Compiler
GCC_VERSION = 4.3.2
CPP = yes
FORTRAN = yes
JAVA = yes

# C Library
LIBC = glibc
LIBC_VERSION = 2.7
THREADING_LIB = nptl

# Tools
LIBELF = yes
OPT_SSTRIP = yes

# Debugging Tools
GDB = yes
DUMA = yes
DMALLOC = yes
LTRACE = yes
STRACE = yes
